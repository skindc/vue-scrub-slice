{
  "name": "@skindc/vue-scrub-slice",
  "version": "0.0.1-beta.1",
  "description": "A scrub bar for slicing an rang from length for Vue.js 3",
  "main": "webpack.config.js",
  "main": "dist/vue-scrub-slice.cjs.js",
  "module": "dist/vue-scrub-slice.es.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build:demo": "vue-cli-service build --dest demo",
    "build:dist": "bili --no-babel.babelrc src/plugin.js",
    "lint": "vue-cli-service lint",
    "build": "npm run build:dist && npm run build:demo",
    "prepare": "npm run build && npm test",
    "test": "tape -r ./test/helpers/setup.js ./test/unit/*.tap.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skindc/vue-scrub-slice.git"
  },
  "keywords": [
    "vue",
    "component",
    "scrub",
    "bar",
    "slice"
  ],
  "author": "Gary Law <skin@skindc.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skindc/vue-scrub-slice/issues"
  },
  "homepage": "https://github.com/skindc/vue-scrub-slice#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-es2015": "^7.0.0-beta.44",
    "@babel/preset-stage-3": "^7.0.0-beta.44",
    "@vue/babel-preset-app": "^3.0.0-beta.6",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "bili": "^3.0.15",
    "browser-env": "^3.2.5",
    "node-sass": "^4.8.3",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-vue": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-vue2": "^0.8.0",
    "sass-loader": "^6.0.7",
    "sass-resources-loader": "^1.3.3",
    "skindc-require-extension-hooks-babel7": "git+https://github.com/skindc/require-extension-hooks-babel.git",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0"
  },
  "dependencies": {}
}
